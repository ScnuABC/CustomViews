<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.dendy.view">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.dendy.customviews.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.dendy.customviews.activities.CustomSeekBarActivity"></activity>
        <activity android:name="com.dendy.customviews.activities.Q5ChannelBalanceSeekBarActivity"></activity>
        <activity android:name="com.dendy.customviews.activities.VolumeViewActivity"></activity>
        <activity android:name="com.dendy.customviews.activities.M7VolumeViewActivity"></activity>
    </application>

</manifest>